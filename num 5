0101101101100000;and r5
0101110110100000;and r6
0010001000000010;ld n into r1 offset 3
0010011000000001;ld n into r3 offset 2
0000111000000001;branch +1
0000000100111000;n
0010010000000001;ld d into r2 +2
0000111000000010;branch +1
0000000001010001;d
1001101001111111;not r1 into r5
0001101101100001;add 1 to r5
0001101101000010;add r5 and r2 into r5
0000100000000101;branch if positive(n smaller than d) +6
1001010010111111;not r2
0001010010100001;add 1 to r2
0001011011000010;add r2 to r3
0001100100100001;add 1 to r4
0101110110100000;and r6
